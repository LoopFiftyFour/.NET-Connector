@using Loop54.Test.AspNetCore.Models
@model GetIndexedAttributeValuesViewModel
@{
    ViewData["PageName"] = "GetIndexedAttributeValues";
}
<form id="SearchForm" method="POST">
    <div>
        <label for="AttributeName">Attribute name</label>
        <input type="text" id="AttributeName" name="AttributeName" autofocus="" />
        <input type="submit" id="SubmitButton" name="Search" />
    </div>
</form>
@if (Model != null)
{
    <h3>@Model.Values.Length indexed values</h3>
    <ol>
        @foreach (string name in Model.Values)
        {
            <li>@name</li>
        }
    </ol>
}
